{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "import-name": false, // Broken: https://github.com/Microsoft/tslint-microsoft-contrib/issues/378
    "object-literal-sort-keys": false, // Needs auto-fixer
    "no-unused-expression": [true, "allow-fast-null-checks"],
    "no-console": true // Here for debug purposes; Must remain true
  }
}
